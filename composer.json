{
    "name": "survos/inspection-bundle",
    "description": "Inspects Doctrine entities with APIPlatform attributes",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.7 || ^3.0",
        "survos/core-bundle": "^1.2.15",
        "symfony/config": "^6.1.0",
        "symfony/dependency-injection": "^6.1.0",
        "symfony/http-kernel": "^6.1.0",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Survos\\InspectionBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.3-dev"
        },
        "symfony": {
            "require": "^6.1.0"
        }
    },
    "conflict": {
        "survos/maker-bundle": "<1.2.15",
        "survos/html-prettify-bundle": "<1.2.15",
        "survos/barcode-bundle": "<1.2.15",
        "survos/api-grid-bundle": "<1.2.15",
        "survos/second-package": "<1.2.7",
        "survos/bootstrap-bundle": "<1.2.15",
        "survos/faker-bundle": "<1.2.15",
        "survos/doc-bundle": "<1.2.15",
        "survos/grid-bundle": "<1.2.15",
        "survos/tree-bundle": "<1.2.15",
        "survos/first-package": "<1.2.7",
        "survos/auth-bundle": "<1.2.15",
        "survos/workflow-helper-bundle": "<1.2.15",
        "survos/crawler-bundle": "<1.2.15",
        "survos/location-bundle": "<1.2.15"
    }
}
